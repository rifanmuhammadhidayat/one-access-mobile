{
  "expo": {
    "name": "OneAccess by Gagas",
    "slug": "gagas-one-access",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-one-access.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0ba7e0"
    },
    "android": {
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#0ba7e0",
        "mdpi": "./assets/splash.png",
        "hdpi": "./assets/splash.png",
        "xhdpi": "./assets/splash.png",
        "xxhdpi": "./assets/splash.png",
        "xxxhdpi": "./assets/splash.png"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-one-access.png",
        "backgroundColor": "#0ba7e0"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rifanmuhammadhidayat.gagasoneaccess"
    },
    "extra": { "eas": { "projectId": "aeddf969-9ed5-4171-9db9-118813e26d92" } },
    "plugins": [
         [
           "expo-build-properties",
           {
             "android": {
               "compileSdkVersion": 34,
               "targetSdkVersion": 34,
               "buildToolsVersion": "34.0.0"
             }
           }
         ]
       ]
     }
  }

